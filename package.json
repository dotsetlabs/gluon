{
  "name": "@dotsetlabs/gluon",
  "version": "1.0.2",
  "description": "Security SDK for the dotset platform. Runtime telemetry, secret leak detection, and network monitoring.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./monitors": {
      "types": "./dist/monitors/index.d.ts",
      "import": "./dist/monitors/index.js"
    },
    "./monitors/secrets": {
      "types": "./dist/monitors/secrets.d.ts",
      "import": "./dist/monitors/secrets.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry.d.ts",
      "import": "./dist/telemetry.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js"
    },
    "./runner": {
      "types": "./dist/runner.d.ts",
      "import": "./dist/runner.js"
    },
    "./auto": {
      "types": "./dist/auto.d.ts",
      "import": "./dist/auto.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "dotset",
    "security",
    "telemetry",
    "monitoring",
    "runtime",
    "secrets",
    "sdk",
    "library"
  ],
  "author": "dotset labs <hello@dotsetlabs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotsetlabs/gluon.git"
  },
  "bugs": {
    "url": "https://github.com/dotsetlabs/gluon/issues"
  },
  "homepage": "https://dotsetlabs.com",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}